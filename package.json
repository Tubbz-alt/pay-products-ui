{
  "name": "pay-products-ui",
  "description": "Payments products ui application",
  "version": "0.0.1-SNAPSHOT",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">6.12.2"
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ],
    "ignore": [
      "app/assets/**/*.js",
      "node_modules_local/**/*.*"
    ]
  },
  "scripts": {
    "compile": "grunt generate-assets",
    "clean": "grunt clean",
    "start": "node_modules/forever/bin/forever --minUptime 1000 --spinSleepTime 500 start.js",
    "stop": "node_modules/forever/bin/forever stop start.js",
    "watch": "chokidar app test *.js --initial -c 'npm run test'",
    "watch-live-reload": "./node_modules/.bin/grunt watch",
    "lint": "./node_modules/.bin/standard",
    "lint-sass": "./node_modules/.bin/sass-lint -v",
    "test-with-coverage": "node ./node_modules/nyc/bin/nyc npm test",
    "test": "node ./node_modules/mocha/bin/mocha '!(node_modules)/**/*+(_test|_tests)'.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "appmetrics": "3.1.x",
    "appmetrics-statsd": "2.0.x",
    "body-parser": "1.18.x",
    "connect-flash": "^0.1.1",
    "correlation-id": "^2.0.0",
    "express": "4.16.x",
    "forever": "0.15.x",
    "lodash": "4.17.x",
    "minimist": "1.2.x",
    "morgan": "1.9.x",
    "nunjucks": "^3.0.1",
    "request": "^2.83.0",
    "requestretry": "^1.12.0",
    "rfc822-validate": "^1.0.0",
    "serve-favicon": "2.4.5",
    "staticify": "2.0.x",
    "throng": "4.0.x",
    "tunnel": "0.0.5",
    "url-join": "^2.0.2",
    "winston": "2.4.x",
    "cookie-parser": "1.4.x",
    "client-sessions": "^0.8.0",
    "csrf": "3.0.x",
    "currency-formatter": "^1.2.1"
  },
  "devDependencies": {
    "@pact-foundation/pact-node": "4.6.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "chai": "^4.0.2",
    "cheerio": "^1.0.0-rc.1",
    "chokidar-cli": "latest",
    "envfile": "^2.1.1",
    "eslint": "4.18.x",
    "govuk-elements-sass": "3.1.2",
    "govuk_frontend_toolkit": "7.4.2",
    "govuk_template_jinja": "^0.23.0",
    "grunt": "1.0.x",
    "grunt-browserify": "^5.0.0",
    "grunt-cli": "1.2.x",
    "grunt-concurrent": "2.3.x",
    "grunt-contrib-clean": "1.1.x",
    "grunt-contrib-copy": "1.0.x",
    "grunt-contrib-watch": "1.0.x",
    "grunt-nodemon": "0.4.x",
    "grunt-sass": "2.1.0",
    "grunt-text-replace": "0.4.x",
    "html5shiv": "3.7.x",
    "jquery": "^3.2.1",
    "mocha": "5.0.x",
    "nock": "9.2.x",
    "node-sass": "4.7.x",
    "nunjucksify": "^2.2.0",
    "nyc": "^11.3.0",
    "pact": "1.0.0",
    "proxyquire": "~1.8.0",
    "sass-lint": "^1.10.2",
    "sinon": "4.4.x",
    "snyk": "1.69.x",
    "standard": "^11.0.0",
    "supertest": "^3.0.0"
  },
  "snyk": true
}
