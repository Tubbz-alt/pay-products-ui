{
  "name": "pay-products-ui",
  "description": "Payments products ui application",
  "version": "0.0.1-SNAPSHOT",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">6.11.1"
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ],
    "ignore": [
      "app/assets/**/*.js",
      "node_modules_local/**/*.*"
    ]
  },
  "scripts": {
    "compile": "grunt generate-assets",
    "clean": "grunt clean",
    "start": "node_modules/forever/bin/forever --minUptime 1000 --spinSleepTime 500 start.js",
    "stop": "node_modules/forever/bin/forever stop start.js",
    "watch": "chokidar app test *.js --initial -c 'npm run test'",
    "watch-live-reload": "./node_modules/.bin/grunt watch",
    "lint": "./node_modules/.bin/standard",
    "lint-sass": "./node_modules/.bin/sass-lint -v",
    "test": "npm run lint && npm run grunt-test",
    "grunt-test": "node ./node_modules/.bin/grunt test",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "appmetrics": "3.1.x",
    "appmetrics-statsd": "2.0.x",
    "body-parser": "1.18.x",
    "connect-flash": "^0.1.1",
    "correlation-id": "^2.0.0",
    "express": "4.16.x",
    "forever": "0.15.x",
    "lodash": "4.17.x",
    "minimist": "1.2.x",
    "morgan": "1.9.x",
    "nunjucks": "^3.0.1",
    "request": "^2.83.0",
    "requestretry": "^1.12.0",
    "serve-favicon": "2.4.5",
    "staticify": "0.0.x",
    "throng": "4.0.x",
    "tunnel": "0.0.5",
    "url-join": "^2.0.2",
    "winston": "2.4.x"
  },
  "devDependencies": {
    "@pact-foundation/pact-node": "4.6.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "8.0.x",
    "chai": "^4.0.2",
    "cheerio": "^1.0.0-rc.1",
    "chokidar-cli": "latest",
    "eslint": "3.x",
    "govuk-elements-sass": "3.1.2",
    "govuk_frontend_toolkit": "7.2.0",
    "govuk_template_jinja": "^0.23.0",
    "grunt": "1.0.x",
    "grunt-browserify": "^5.0.0",
    "grunt-cli": "1.2.x",
    "grunt-concurrent": "2.3.x",
    "grunt-contrib-clean": "1.1.x",
    "grunt-contrib-copy": "1.0.x",
    "grunt-contrib-watch": "1.0.x",
    "grunt-env": "0.4.x",
    "grunt-mocha-test": "^0.13.2",
    "grunt-nodemon": "0.4.x",
    "grunt-sass": "2.0.x",
    "grunt-text-replace": "0.4.x",
    "html5shiv": "3.7.x",
    "jquery": "^3.2.1",
    "mocha": "4.0.x",
    "nock": "9.1.x",
    "node-sass": "4.7.x",
    "pact": "1.0.0",
    "proxyquire": "~1.8.0",
    "sass-lint": "^1.10.2",
    "sinon": "4.1.x",
    "snyk": "1.54.x",
    "standard": "^10.0.2",
    "supertest": "^3.0.0"
  },
  "snyk": true
}
